class Solution {
public:
    int numOfUnplacedFruits(vector<int>& fruits, vector<int>& baskets) {
        int n=fruits.size();
        vector<int> arr(n,-1);
        int count=n;
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                if(fruits[i]<=baskets[j] && arr[j]==-1){
                    arr[j]=0;
                    count--;
                    break;
                }
            }
        }
        return count;
    }
};