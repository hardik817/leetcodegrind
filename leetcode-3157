class Solution
{
public:
    vector<int> findLonely(vector<int> &nums)
    {
        unordered_map<int, int> mp;
        vector<int> ans;
        for (int i : nums)
        {
            mp[i]++;
        }
        for (int i : nums)
        {
            if (mp[i] == 1 && !mp.count(i - 1) && !mp.count(i + 1))
            {
                ans.push_back(i);
            }
        }

        return ans;
    }
};